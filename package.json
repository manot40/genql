{
  "type": "module",
  "name": "@manot40/genql-cli",
  "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
  "license": "MIT",
  "version": "0.0.0",
  "homepage": "https://genql.dev",
  "description": "Generate a TypeScript SDK for any GraphQl API",
  "keywords": [
    "genql",
    "graphql",
    "sdk",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/manot40/genql"
  },
  "bin": {
    "genql": "./cli.mjs"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist esm *.tsbuildinfo && tsdown",
    "test": "jest"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^30.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mkdirp": "^2.0.0",
    "@types/node": "^18.19.130",
    "@types/prettier": "^3.0.0",
    "@types/rimraf": "^4.0.5",
    "@types/yargs": "^17.0.35",
    "changelogen": "^0.6.2",
    "common-tags": "^1.8.2",
    "jest": "^29.3.1",
    "prettier": "^3.7.4",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.4.6",
    "tsdown": "^0.19.0-beta.3"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.1.9",
    "@graphql-tools/load": "^8.1.8",
    "fs-extra": "^11.3.3",
    "graphql": "^16.12.0",
    "kleur": "^4.1.5",
    "listr2": "^9.0.5",
    "lodash-es": "^4.17.22",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.1.2",
    "ufo": "^1.6.2",
    "undici": "^7.18.2",
    "yargs": "^18.0.0"
  },
  "peerDependencies": {
    "prettier": "3.x"
  },
  "jest": {
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
