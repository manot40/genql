{
  "type": "module",
  "name": "@manot40/genql-cli",
  "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
  "license": "MIT",
  "version": "1.1.0",
  "homepage": "https://genql.dev",
  "description": "Generate a TypeScript SDK for any GraphQl API",
  "keywords": [
    "genql",
    "graphql",
    "sdk",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/manot40/genql"
  },
  "bin": {
    "genql": "./dist/cli.mjs"
  },
  "engines": {
    "node": ">= 22.x"
  },
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "jest",
    "prettier:write": "prettier -w .",
    "prettier:check": "prettier -c ."
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.1.9",
    "@graphql-tools/load": "^8.1.8"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "@types/yargs": "^17.0.35",
    "changelogen": "^0.6.2",
    "common-tags": "^1.8.2",
    "jest": "^29.3.1",
    "kleur": "^4.1.5",
    "listr2": "^9.0.5",
    "lodash-es": "^4.17.22",
    "ts-jest": "^29.4.6",
    "tsdown": "^0.19.0-beta.5",
    "yargs": "^18.0.0"
  },
  "peerDependencies": {
    "graphql": "^16.12.0",
    "prettier": "^3.7.4"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
